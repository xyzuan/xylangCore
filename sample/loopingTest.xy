print "= Bubble Sort ======="
fun bubble_sort [arr, length]
    loop i in 0..length 1
        loop j in 0..length - i - 1
            if arr{j+1} < arr{j}
                temp = arr{j}
                arr{j} = arr{j + 1}
                arr{j + 1} = temp
            end
       end
    end
end

arr = {2, 5, 6, 7, 19, 1, 2, 5, 8}
bubble_sort[arr,9]
print arr
